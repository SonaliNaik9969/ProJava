package dynamic_poly;

class Data
{
	public void display(int x)
	{
		System.out.println("X :"+x);
	}
	public void finalize()
	{
		System.out.println("Finalize method of class Data");
	}
}
public class Finalize_1 {
	public static void main(String[] args) {
		Data d = new Data();
		d.display(10);
		
		System.gc();
	}

}
